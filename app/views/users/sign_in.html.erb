<h1>登入</h1>
<%= form_with model: @user, url: sessions_path do |form| %>
  <div class="field">
    <%= form.label :email, "Email" %>
    <%= form.text_field :email %>
  </div>
  <div class="field">
    <%= form.label :password, "密碼" %>
    <%= form.password_field :password %>
  </div>
  <%= form.submit "登入" ,class: 'btn btn-primary'%>
<% end %>
